<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.linuxtools.dataviewers.feature"
      label="%featureName"
      version="0.10.0.qualifier"
      provider-name="%featureProvider">

   <description url="http://www.example.com/description">
      %featureDescription
   </description>

   <copyright url="http://www.example.com/copyright">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="Linux Tools Update Site" url="http://download.eclipse.org/technology/linuxtools/update"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.cdt.ui"/>
      <import plugin="org.eclipse.cdt.core"/>
      <import plugin="org.eclipse.linuxtools.dataviewers" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.birt.core" version="2.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.birt.chart.ui" version="2.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.birt.data" version="2.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.birt.data.aggregation" version="2.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.4.2" match="greaterOrEqual"/>
      <import plugin="com.ibm.icu"/>
   </requires>

   <plugin
         id="org.eclipse.linuxtools.dataviewers"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.linuxtools.dataviewers.annotatedsourceeditor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.linuxtools.dataviewers.charts"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
